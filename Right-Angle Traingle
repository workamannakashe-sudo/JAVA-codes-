import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();  // Input for size of triangle

        for (int i = 1; i <= n; i++) {
            // print i stars in each row
            for (int j = 1; j <= i; j++) {
                System.out.print("* ");
            }
            System.out.println();  // move to next line
        }
    }
}
